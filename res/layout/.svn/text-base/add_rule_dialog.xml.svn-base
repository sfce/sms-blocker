<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/dialog" android:background="#ffffffff" android:orientation="vertical" android:padding="5.0dip" android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <TextView android:textAppearance="?android:textAppearanceMedium" android:id="@+id/lblKeyWord" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="@string/label_keyword" />
    <EditText android:id="@+id/inputTxtKeyWord" android:layout_width="fill_parent" android:layout_height="wrap_content" android:ems="10" android:inputType="text">
        <requestFocus />
    </EditText>
    <TextView android:textAppearance="?android:textAppearanceMedium" android:id="@+id/lblRuleType" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="@string/label_match_rule" />
    <Spinner android:id="@+id/spinRuleType" android:layout_width="fill_parent" android:layout_height="wrap_content" />
    <TextView android:textAppearance="?android:textAppearanceMedium" android:id="@+id/lblRuleTypeTipe" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="匹配规则说明请查看帮助" />
   
</LinearLayout>
